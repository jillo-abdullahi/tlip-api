    "name": {
      "type": "string"
    },
    "color": {
      "type": "string"
    },
    "price": {
      "type": "number"
    },
    "quantity": {
      "type": "number"
    },
    "description": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "postalCode": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "shelfLife": {
      "type": "number"
    },
    "safetyStock": {
      "type": "number"
    },
    "country": {
      "type": "string"
    }
  },
  "required": ["name"],
  "additionalProperties": false
}
